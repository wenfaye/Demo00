KISSY.add("jbc/jupicker/2.1.8/item/crossshop/index",function(o,r,n,i){return{renderItems:function(r,n){var i=this,t="";return o.each(n,function(o,n){t+=i.render(r,o)}),t},render:function(o,r){var t="";return~location.search.indexOf("crossShopMock=true")&&(r.promotion={marketing:{crossShop:{icon:"//img.alicdn.com/tps/TB1dw4YLXXXXXcXaXXXXXXXXXXX-120-26.png",prom:"满200减10,满300减20,满400减30"}}}),r.promotion&&r.promotion.marketing&&r.promotion.marketing.crossShop&&(r.promotion.marketing.crossShop.prom=r.promotion.marketing.crossShop.prom.split(",")[0]),t+="l"==o?n({item:r}):i({item:r})}}},{requires:["io","./mods/big.tpl","./mods/small.tpl"]});KISSY.add("jbc/jupicker/2.1.8/brand/crossshop/index",function(r,o,n,i){return{renderBrands:function(o,n){var i=this,e="";return r.each(n,function(r,n){e+=i.render(o,r)}),e},render:function(r,o){var e="";return~location.search.indexOf("crossShopMock=true")&&(o.promotion=o.promotion||{},o.promotion.crossShop={prom:["满200减10","满300减20","满400减30"],icon:"//img.alicdn.com/tps/TB1dw4YLXXXXXcXaXXXXXXXXXXX-120-26.png"}),e+="l"==r?n({brand:o}):i({brand:o})}}},{requires:["io","./mods/big.tpl","./mods/middle.tpl"]});